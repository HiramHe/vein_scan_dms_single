<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="hiram.hospital.mapper.BedMapper">

    <delete id="deleteByIdPermanently">
        delete from hos_bed
        where bed_id = #{bedId}
    </delete>

</mapper>