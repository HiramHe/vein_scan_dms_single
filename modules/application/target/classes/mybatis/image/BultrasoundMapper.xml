<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="hiram.image.mapper.BultrasoundMapper">

    <delete id="deleteByIdPermanently">
        delete from img_bultrasound
        where bultrasound_id = #{bultrasoundId}
    </delete>

</mapper>